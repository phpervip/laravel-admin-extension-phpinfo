{
    "name": "phpervip/laravel-admin-extension-phpinfo",
    "description": "Show php information in laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/phpervip/laravel-admin-extension-phpinfo",
    "license": "MIT",
    "authors": [
        {
            "name": "phpervip",
            "email": "2998658517@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\PHPInfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\PHPInfo\\PHPInfoServiceProvider"
            ]
        }
    }
}
